	// });

	////////   PC5   ////////

	if(!getItemFromGlobalLocalStorage('PC_CatAddToBag')){

		var mostViewedCatObj = {
			'C1' : 0,
			'C2' : 0,
			'C3' : 0,
			'C4' : 0,
			'C5' : 0,
			'C6' : 0,
			'C7' : 0,
			'C8' : 0,
			'C9' : 0,
